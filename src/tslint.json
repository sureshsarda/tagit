{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "aye",
      "kebab-case"
    ],
    "variable-name": [
        true,
        "allow-leading-underscore"
    ]
  }
}
